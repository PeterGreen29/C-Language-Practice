#include <stdio.h>
int main(void){
    int num;
    scanf("%d",&num);
    printf("0x%d = %d\n",num,0*num);
    printf("1x%d = %d\n",num,1*num);
    printf("2x%d = %d\n",num,2*num);
    printf("3x%d = %d\n",num,3*num);
    printf("4x%d = %d\n",num,4*num);
    printf("5x%d = %d\n",num,5*num);
    printf("6x%d = %d\n",num,6*num);
    printf("7x%d = %d\n",num,7*num);
    printf("8x%d = %d\n",num,8*num);
    printf("9x%d = %d\n",num,9*num);
    printf("10x%d = %d\n",num,10*num);
    return 0;
}


// I realize that this is not the most efficient way to write this code.
// Below I will try again, but this time using a for loop


#include <stdio.h>
int main(void){
    int i, num, multi = 0;
    scanf("%d",&num);
    for(i=0;i<11;i++){
        printf("%dx%d = %d\n",multi,num,multi*num);
        multi=multi+1;
    }
    return 0;
}

//this code does the same thing as the one above, however in fewer lines & is more efficient with the multiple variables & for loop.
